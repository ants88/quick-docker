{
  "identifier": "default",
  "description": "Default capabilities for QuickDocker",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "shell:allow-execute",
    "shell:allow-spawn",
    "shell:allow-stdin-write",
    "shell:allow-kill",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "quickdocker-backend",
          "sidecar": true
        }
      ]
    }
  ]
}
